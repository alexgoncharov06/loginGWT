<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="default">
    <g:HTMLPanel ui:field="loginForm">
        <form id="form" class="parent">
            <table class="loginForm">
                <tr>
                    <td>
                        <g:Label ui:field="labelLogin"/>
                    </td>
                    <td>
                        <g:TextBox ui:field="loginBox"/>
                    </td>
                </tr>
                <tr>
                    <td>
                        <g:Label ui:field="labelPassword"/>
                    </td>
                    <td>
                        <g:PasswordTextBox ui:field="passwordBox"/>
                    </td>
                </tr>
                <tr>
                    <td>
                    </td>
                    <td>
                        <g:Button ui:field="buttonSubmit"></g:Button>
                    </td>
                </tr>

            </table>
        </form>
    </g:HTMLPanel>

</ui:UiBinder>